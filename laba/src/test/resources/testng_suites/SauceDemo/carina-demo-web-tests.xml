<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite verbose="1" name="Carina Demo Tests - Web Tests" parallel="methods" thread-count="3">
    <parameter name="suiteOwner" value="qpsdemo"/>
    <parameter name="jenkinsJobName" value="Sauce-Demo-Test"/>
    <parameter name="jenkinsJobType" value="web"/>

    <parameter name="jenkinsEnvironments" value="DEMO"/>
    <parameter name="jenkinsSlackChannels" value="slack-test,slack-test2"/>

    <parameter name="jenkinsRegressionPipeline" value="web_regression, full_regression"/>
    <parameter name="jenkinsRegressionScheduling_api_regression" value="H 2 * * *"/>

    <parameter name="jenkinsJobExecutionMode" value="continue"/>

    <test name="Sauce Demo Test">
        <classes>
          <class name="laba.carina.demo.SauceDemoTest">
            <methods>
                <include name="testProductAddedToCart"/>
                <include name="testAddToCartAndCheckout"/>
                <include name="testClickOnAboutPage"/>
                <include name="testSortProductsAndAddToCart"/>
                <include name="testLogOutFromProductsPage"/>
            </methods>
          </class>
        </classes>
    </test>

</suite>